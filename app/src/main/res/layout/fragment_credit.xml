<!-- credit_fragment.xml -->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical"
		android:padding="16dp">

	<TextView
			android:id="@+id/philippinePesoCreditTextView"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textSize="16sp"
			android:layout_marginBottom="12dp"/>

	<TextView
			android:id="@+id/learningCreditTextView"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textSize="16sp"
			android:layout_marginBottom="12dp"/>

	<TextView
			android:id="@+id/testCreditTextView"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textSize="16sp"
			android:layout_marginBottom="12dp"/>

	<TextView
			android:id="@+id/stackOfBooksCreditTextView"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textSize="16sp"
			android:layout_marginBottom="12dp"/>

	<TextView
			android:id="@+id/openBookCreditTextView"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textSize="16sp"
			android:layout_marginBottom="12dp"/>

	<TextView
			android:id="@+id/readCreditTextView"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textSize="16sp"
			android:layout_marginBottom="12dp"/>

	<TextView
			android:id="@+id/searchCreditTextView"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:textSize="16sp"
			android:layout_marginBottom="12dp"/>

</LinearLayout>
