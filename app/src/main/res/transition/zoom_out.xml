<?xml version="1.0" encoding="utf-8"?>
<transitionSet
		xmlns:android = "http://schemas.android.com/apk/res/android">
	<transition>
		<scale
				android:duration = "200"
				android:fromXScale = "1"
				android:fromYScale = "1"
				android:toXScale = "0.8"
				android:toYScale = "0.8"
				android:pivotX = "50%"
				android:pivotY = "50%" />
	</transition>
</transitionSet>